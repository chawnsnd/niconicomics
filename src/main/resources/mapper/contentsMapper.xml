<!-- 콘텐츠 처리 SQL -->
<mapper namespace="com.niconicomics.core.dao.ContentsMapper">

<!-- 콘텐츠 입력 -->
	<insert id="contentsInsert" parameterType="contents">
		insert into tbl_contents(
			contents_id
			,episode_id
			,idx
			,image
		)values(
			seq_contents.nextval
			,#{episodeId}
			,#{idx}
			,#{image}
		)
	</insert>

	
		
